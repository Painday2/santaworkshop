		<body name="body_animated" enabled="true" template="animated">
			<object name="anim_door" />
			<object name="c_box_05" collision_type="box" />
		</body>
	</bodies>
	<decal_surfaces default_material="metal">
		<decal_mesh name="dm_metal_frame" enabled="true" material="sheet_metal" />
		<decal_mesh name="dm_metal_door" enabled="true" material="sheet_metal" />
		<decal_mesh name="dm_glass" enabled="true" material="glass_unbreakable" />
	</decal_surfaces>
	<graphics>
		<object name="g_frame" enabled="true" />
		<object name="g_door" enabled="true" />
	</graphics>
</dynamic_object>
