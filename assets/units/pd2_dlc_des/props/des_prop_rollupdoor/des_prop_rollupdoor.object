<dynamic_object>
	<diesel materials="units/pd2_dlc_des/props/des_prop_rollupdoor/des_prop_rollupdoor" orientation_object="rp_des_prop_rollupdoor" />
	<sequence_manager file="units/pd2_dlc_des/props/des_prop_rollupdoor/des_prop_rollupdoor" />
	<animations>
		<animation_group name="door" enabled="false">
			<object name="anim_door" />
